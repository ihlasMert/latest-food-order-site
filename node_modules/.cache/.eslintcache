unt":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"219","usedDeprecatedRules":"186"},{"filePath":"220","messages":"221","suppressedMessages":"222","errorCount":0,"fatalErrorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"223","usedDeprecatedRules":"186"},{"filePath":"224","messages":"225","suppressedMessages":"226","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"186"},{"filePath":"227","messages":"228","suppressedMessages":"229","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"186"},{"filePath":"230","messages":"231","suppressedMessages":"232","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"186"},{"filePath":"233","messages":"234","suppressedMessages":"235","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"236","messages":"237","suppressedMessages":"238","errorCount":0,"fatalErrorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"239","usedDeprecatedRules":"186"},{"filePath":"240","messages":"241","suppressedMessages":"242","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"186"},"/Users/ihlasmert/Desktop/01-starting-project/src/index.js",[],[],[],"/Users/ihlasmert/Desktop/01-starting-project/src/App.js",[],[],"/Users/ihlasmert/Desktop/01-starting-project/src/pages/Home.jsx",[],[],"/Users/ihlasmert/Desktop/01-starting-project/src/components/header/Header.jsx",[],[],"/Users/ihlasmert/Desktop/01-starting-project/src/assets/fake-data/slider.js",[],[],"/Users/ihlasmert/Desktop/01-starting-project/src/components/popular-menu/PopularMenu.jsx",["243"],[],"/Users/ihlasmert/Desktop/01-starting-project/src/assets/fake-data/products.js",[],[],"/Users/ihlasmert/Desktop/01-starting-project/src/components/menu-pack/MenuPack.jsx",[],[],"/Users/ihlasmert/Desktop/01-starting-project 3/src/index.js",[],[],"/Users/ihlasmert/Desktop/01-starting-project 3/src/App.js",[],[],[],"/Users/ihlasmert/Desktop/01-starting-project 3/src/components/Layout/Layout.js",[],[],"/Users/ihlasmert/Desktop/01-starting-project 3/src/components/Footer/Footer.jsx",[],[],"/Users/ihlasmert/Desktop/01-starting-project 3/src/components/Header/Header.jsx",[],[],"/Users/ihlasmert/Desktop/01-starting-project 3/src/routers/Routers.js",[],[],"/Users/ihlasmert/Desktop/01-starting-project 3/src/pages/FoodDetails.jsx",[],[],"/Users/ihlasmert/Desktop/01-starting-project 3/src/pages/Checkout.jsx",[],[],"/Users/ihlasmert/Desktop/01-starting-project 3/src/pages/Login.jsx",[],[],"/Users/ihlasmert/Desktop/01-starting-project 3/src/pages/Register.jsx",[],[],"/Users/ihlasmert/Desktop/01-starting-project 3/src/pages/Home.jsx",["244"],[],"import React, { useEffect, useState } from \"react\"\nimport Helmet from \"../components/Helmet/Helmet\"\nimport { Container, Row, Col } from \"reactstrap\"\nimport \"../styles/hero-section.css\"\nimport { Link } from \"react-router-dom\"\nimport \"../../src/styles/home.css\"\n\nimport heroImg from \"../assets/images/hero.png\"\nimport Category from \"../UI/category/Category\"\n\nimport featureImg01 from \"../../src/assets/images/service-01.png\"\nimport featureImg02 from \"../../src/assets/images/service-02.png\"\nimport featureImg03 from \"../../src/assets/images/service-03.png\"\nimport products from \"../assets/fake-data/products.js\"\nimport ProductCard from \"../../src/UI/product-card/ProductCard\"\n\nconst featureDate = [\n  { title: \"Quick Delivery\", imgUrl: featureImg01, desc: \"lorem lorem\" },\n  { title: \"Quick Delivery\", imgUrl: featureImg02, desc: \"lorem lorem\" },\n  { title: \"Pick Up\", imgUrl: featureImg03, desc: \"lorem lorem\" }\n]\n\nconst Home = () => {\n  const [category, setCategory] = useState(\"ALL\")\n  const [allProducts, setAllProducts] = useState(products)\n  const [hotPizza, setHotPizza] = useState([])\n\n  useEffect(() => {\n    const filteredPizza = products.filter((item) => item.category === \"Pizza\")\n    const slicePizza = filteredPizza.slice(0, 3)\n    setHotPizza(slicePizza)\n  }, [])\n\n  useEffect(() => {\n    if (category === \"ALL\") {\n      setAllProducts(products)\n    }\n    if (category === \"BURGER\") {\n      const filteredProducts = products.filter(\n        (item) => item.category === \"Burger\"\n      )\n      setAllProducts(filteredProducts)\n    }\n    if (category === \"PIZZA\") {\n      const filteredProducts = products.filter(\n        (item) => item.category === \"Pizza\"\n      )\n      setAllProducts(filteredProducts)\n    }\n    if (category === \"BREAD\") {\n      const filteredProducts = products.filter(\n        (item) => item.category === \"Bread\"\n      )\n      setAllProducts(filteredProducts)\n    }\n    if (category === \"LEMON\") {\n      const filteredProducts = products.filter(\n        (item) => item.category === \"Lemon\"\n      )\n      setAllProducts(filteredProducts)\n    }\n  }, [category])\n  return (\n    <Helmet title='Home'>\n      <section>\n        <Container>\n          <Row>\n            <Col lg='6' md='6'>\n              <div className='hero__content'>\n                <h5 className='mb-3'>Ease way to make an order</h5>\n                <h1 className='mb-4 hero__title'>\n                  <span>Hungry?</span>\n                  <span>just wait food at your door</span>\n                </h1>\n                <p>\n                  Lorem ipsum dolor sit amet consectetur adipisicing elit.\n                  Voluptatum, repudiandae.\n                </p>\n                <div className='hero__btns d-flex align-items-center gap-5 mt-4'>\n                  <button className='order__btn d-flex align-item-center justify-content-between'>\n                    Order now\n                    <i class='ri-arrow-right-s-line'></i>\n                  </button>\n                  <button className='all__foods-btn'>\n                    <Link to='/foods'>See all foods</Link>\n                  </button>\n                </div>\n              </div>\n            </Col>\n            <Col lg='6' md='6'>\n              <div className='hero__img'>\n                <img src={heroImg} alt='hero image' className='w-100' />\n              </div>\n            </Col>\n          </Row>\n        </Container>\n      </section>\n      <section>\n        <Category />\n      </section>\n      <section>\n        <Container>\n          <Row>\n            <Col lg='12' className='text-center'>\n              <h5 className='feature__subtitle'>What we serve?</h5>\n              <h2 className='feature__title'>Just sit back at home</h2>\n              <h2 className='feature__title'>\n                We will <span>take care</span>\n              </h2>\n              <p className='mb-1 mt-4 feature__text'>\n                Lorem ipsum dolor sit amet consectetur adipisicing elit. Illo,\n                fuga.\n              </p>\n              <p className='feature__text'>\n                Lorem ipsum dolor sit amet consectetur adipisicing elit. Illo,\n                fuga.\n              </p>\n            </Col>\n            {featureDate.map((item, index) => (\n              <Col lg='4' md='4' key={index} className='mt-5'>\n                <div className='feature__item text-center px-5 py-3'>\n                  <img src={item.imgUrl} alt='feature-img' className='w-25' />\n                  <h5 className='fw-bold'>{item.title}</h5>\n                  <h5>{item.desc}</h5>\n                </div>\n              </Col>\n            ))}\n          </Row>\n        </Container>\n      </section>\n      <section>\n        <Container>\n          <Row>\n            <Col lg='12' className='text-center'>\n              <h2>Popular Foods</h2>\n            </Col>\n            <Col lg='12'>\n              <div className='food__category d-flex align-items-center justify-content-center gap-4'>\n                <button\n                  className={`all__btn  ${\n                    category === \"ALL\" ? \"foodBtnACtive\" : \"\"\n                  }`}\n                  onClick={() => setCategory(\"ALL\")}\n                >\n                  All\n                </button>\n                <button\n                  className={`d-flex align-items-center gap-2 ${\n                    category === \"BURGER\" ? \"foodBtnACtive\" : \" \"\n                  }`}\n                  onClick={() => setCategory(\"BURGER\")}\n                >\n                  Burger\n                </button>\n                <button\n                  className={`d-flex align-items-center gap-2 ${\n                    category === \"PIZZA\" ? \"foodBtnACtive\" : \" \"\n                  }`}\n                  onClick={() => setCategory(\"PIZZA\")}\n                >\n                  Pizza\n                </button>\n                <button\n                  className={`d-flex align-items-center gap-2 ${\n                    category === \"BREAD\" ? \"foodBtnACtive\" : \"\"\n                  }`}\n                  onClick={() => setCategory(\"BREAD\")}\n                >\n                  Bread\n                </button>\n              </div>\n            </Col>\n            {allProducts.map((item) => (\n              <Col lg='3' md='4' sm='6' xs='6' key={item.id} className='mt-5'>\n                <ProductCard item={item} />\n              </Col>\n            ))}\n          </Row>\n        </Container>\n      </section>\n\n      <section>\n        <Container>\n          <Row>\n            <Col lg='12' className='text-center '>\n              <h2>Hot Pizza</h2>\n            </Col>\n            {hotPizza.map((item) => (\n              <Col lg='3' md='4' key={item.id}>\n                <ProductCard item={item} />\n              </Col>\n            ))}\n          </Row>\n        </Container>\n      </section>\n    </Helmet>\n  )\n}\n\nexport default Home\n","/Users/ihlasmert/Desktop/01-starting-project 3/src/pages/Cart.jsx",[],[],"/Users/ihlasmert/Desktop/01-starting-project 3/src/pages/Contact.jsx",[],[],"/Users/ihlasmert/Desktop/01-starting-project 3/src/pages/AllFoods.jsx",[],[],"/Users/ihlasmert/Desktop/01-starting-project 3/src/components/Helmet/Helmet.js",[],[],"/Users/ihlasmert/Desktop/01-starting-project 3/src/UI/category/Category.jsx",[],[],"/Users/ihlasmert/Desktop/01-starting-project 3/src/assets/fake-data/products.js",[],[],"/Users/ihlasmert/Desktop/01-starting-project 3/src/UI/product-card/ProductCard.jsx",[],[],"/Users/ihlasmert/Desktop/01-starting-project 3/src/UI/cart/Cart.jsx",["245","246"],[],"/Users/ihlasmert/Desktop/01-starting-project 3/src/UI/cart/CartItem.jsx",[],[],"/Users/ihlasmert/Desktop/food-order-web-main/src/index.js",[],[],[],"/Users/ihlasmert/Desktop/food-order-web-main/src/App.js",[],[],"/Users/ihlasmert/Desktop/food-order-web-main/src/store/store.js",[],[],"/Users/ihlasmert/Desktop/food-order-web-main/src/components/Layout/Layout.js",[],[],"/Users/ihlasmert/Desktop/food-order-web-main/src/store/shopping-Cart/cartSlice.js",["247","248"],[],"import React from \"react\"\nimport { createSlice, current } from \"@reduxjs/toolkit\"\n\nconst initialState = {\n  cartItems: [],\n  totalQuantity: 0,\n  totalAmount: 0\n}\n\nconst cartSlice = createSlice({\n  name: \"cart\",\n  initialState: initialState,\n\n  reducers: {\n    addItem(state, action) {\n      const newItem = action.payload\n      const existingItem = state.cartItems.find(\n        (item) => item.id === newItem.id\n      )\n      state.totalQuantity++\n\n      if (!existingItem) {\n        state.cartItems.push({\n          id: newItem.id,\n          title: newItem.title,\n          image01: newItem.image01,\n          price: newItem.price,\n          quantity: 1,\n          totalPrice: newItem.price\n        })\n      } else {\n        existingItem.quantity++\n        existingItem.totalPrice =\n          Number(existingItem.totalPrice) + Number(newItem.price)\n      }\n      state.totalAmount = state.cartItems.reduce(\n        (total, item) => total + Number(item.price) * Number(item.quantity),\n        0\n      )\n    },\n    /* ===remove item=== */\n    removeItem(state, action) {\n      const id = action.payload\n      const existingItem = state.cartItems.find((item) => item.id === id)\n      state.totalQuantity--\n      if (existingItem.quantity === 1) {\n        state.cartItems = state.cartItems.filter((item) => item.id !== id)\n      } else {\n        existingItem.quantity--\n        existingItem.totalPrice =\n          Number(existingItem.totalPrice) - Number(existingItem.price)\n      }\n      state.totalAmount = state.cartItems.reduce(\n        (total, item) => total + Number(item.price) * Number(item.quantity),\n        0\n      )\n    },\n    /* ====delete ıtem=== */\n    deleteItem(state, action) {\n      const id = action.payload\n      const existingItem = state.cartItems.find((item) => item.id === id)\n      if (existingItem) {\n        state.cartItems = state.cartItems.filter((item) => item.id !== id)\n        state.totalQuantity = state.totalQuantity - existingItem.quantity\n      }\n      state.totalAmount = state.cartItems.reduce(\n        (total, item) => total + Number(item.price) * Number(item.quantity),\n        0\n      )\n    }\n  }\n})\n\nexport const cartActions = cartSlice.actions\nexport default cartSlice\n","/Users/ihlasmert/Desktop/food-order-web-main/src/store/shopping-Cart/cartUiSlice.js",[],[],"/Users/ihlasmert/Desktop/food-order-web-main/src/routers/Routers.js",[],[],"/Users/ihlasmert/Desktop/food-order-web-main/src/components/Footer/Footer.jsx",[],[],"/Users/ihlasmert/Desktop/food-order-web-main/src/components/UI/cart/Carts.jsx",[],[],"/Users/ihlasmert/Desktop/food-order-web-main/src/pages/AllFoods.jsx",["249","250"],[],"import React, { useState } from \"react\"\nimport Helmet from \"../compone