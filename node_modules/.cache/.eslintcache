   <div className='navigation' ref={menuRef}>\n            <div className='menu d-flex align-items-center gap-4'>\n              {nav__links.map((item, index) => (\n                <NavLink\n                  onClick={toggleMenu}\n                  to={item.path}\n                  key={index}\n                  className={(navClass) =>\n                    navClass.isActive ? \"active__menu\" : \"\"\n                  }\n                >\n                  {item.display}\n                </NavLink>\n              ))}\n            </div>\n          </div>\n          {/* ?=======nav right icons */}\n          <div className='nav__right d-flex align-items-center gap-4'>\n            <span className='cart__icon' onClick={toggleCart}>\n              <i class='ri-shopping-bag-line'></i>\n              <span className='cart__badge'>{totalQuantity}</span>\n            </span>\n            <span className='user'>\n         \n            </span>\n            <span className='mobile__menu' onClick={toggleMenu}>\n              <i class='ri-menu-line'></i>\n            </span>\n          </div>\n        </div>\n      </Container>\n    </header>\n  )\n}\n\nexport default Header\n","/Users/ihlasmert/Desktop/food-order-web-main/src/components/UI/commonSection/CommonSection.jsx",[],[],{"ruleId":"254","severity":1,"message":"255","line":16,"column":19,"nodeType":"256","endLine":16,"endColumn":62},{"ruleId":"257","severity":1,"message":"258","line":92,"column":17,"nodeType":"256","endLine":92,"endColumn":73},{"ruleId":"259","severity":2,"message":"260","line":18,"column":12,"nodeType":"261","messageId":"262","endLine":18,"endColumn":24},{"ruleId":"259","severity":2,"message":"260","line":21,"column":13,"nodeType":"261","messageId":"262","endLine":21,"endColumn":25},{"ruleId":"263","severity":1,"message":"264","line":1,"column":8,"nodeType":"261","messageId":"265","endLine":1,"endColumn":13},{"ruleId":"263","severity":1,"message":"266","line":2,"column":23,"nodeType":"261","messageId